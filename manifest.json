{
    "manifest_version": 2,
    "name": "CRYPT",
    "description": "CRYPT IT",
    "version": "1.0",
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "CRYPT IT"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage",
        "activeTab",
        "identity",
        "identity.email",
        "http://crypt.dev/crypt/public/api/bookmarks/plugin"
    ]
}